<html>
<head>
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">
  <script src="http://code.jquery.com/jquery-2.2.4.min.js"></script>
  <style>
    body {
      background-color: #ecedec;
    }
  </style>
</head>
<body>
      <div class="jumbotron text-center">
    <h1><span class="glyphicon glyphicon-shopping-cart"></span></h1>
    <h2>Latest models to Buy</h2>
  </div>
    <div class="row">
  <!-- This is the EJS syntax to create a loop -->
  <% buy.forEach(function(buy, index) { %>
    <div class="col-sm-4">
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title"><%= buy.title %> (<%= buy.release %>)</h3>
        </div>
        <div class="panel-body text-center">
          <h1><%= buy.score %> <small>/ 10</small></h1>
        </div>
        <div class="panel-footer small">
          <p>$ <%= buy.price %>USD for <%= buy.description %>
          <a href="https://www.pse.com.co/inicio" class="btn btn-block btn-primary">BUY</a>
        </div>
        
      </div>
    </div>
  <!-- Weâ€™ll also have to close our loop -->
  <% }) %>
  </div>
</body> 
</html>